<?xml version="1.0" encoding="utf-8"?>
<component name="MainScene" extends="Scene">
    <interface>
        <field id="inputArgs" type="assocarray" onChange="OnInputDeepLinking" />
        <field id="deepLinkHandled" type="boolean" value="false" onChange="PlayDeepLinkMedia" />
        <field id="episodeHandled" type="boolean" value="false" onChange="PlayDeepLinkMedia" />
        <field id="launchArgs" type="assocarray" onChange="InitDeepLinkLaunch" />
    </interface>
    <script type="text/brightscript" uri="pkg:/components/mainscene.brs"/>
    <children>
        <Timer
            id="timer"
            repeat="true"
            duration="30"
        />
    </children>
</component>
